{
    "manifest_version": 3,
    "name": "Text Black Hole (All Elements)",
    "version": "1.0",
    "description": "吸附网页所有元素的黑洞效果",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_idle"
        }
    ]
}